<%= form_with(model: @property, local: true) do |form| %>

  <%= form.label :名前 %>
  <%= form.hidden_field :name %>

  <%= form.label :賃料 %>
  <%= form.hidden_field :rent %>

  <%= form.label :住所 %>
  <%= form.hidden_field :address %>

  <%= form.label :築年数 %>
  <%= form.hidden_field :year %>

  <%= form.label :備考 %>
  <%= form.hidden_field :content %>

  <%= link_to '編集', edit_property_path(@property.id) %> |
  <%= link_to '戻る', properties_path %>

<% end %>
